{
    "name": "download-github-code",
    "version": "1.0.0",
    "description": "Download a GitHub project source code (only the code, without the .git data)",
    "main": "out/index.js",
    "types": "out/index.d.ts",
    "scripts": {
        "start": "node out/index.js",
        "test": "jest -i --watch",
        "test-web": "gulp && serve tests/web",
        "prepublishOnly": "tsc && gulp"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/diegozanon/download-github-code"
    },
    "keywords": [
        "download",
        "github",
        "repo",
        "repository",
        "source code",
        "zip"
    ],
    "engines": {
        "node": ">= 10.0.0"
    },
    "author": "Diego Zanon",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/diegozanon/download-github-code/issues"
    },
    "homepage": "https://github.com/diegozanon/download-github-code#readme",
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testMatch": [
            "<rootDir>/tests/*.ts"
        ],
        "testRunner": "jest-circus/runner"
    },
    "devDependencies": {
        "@babel/core": "^7.12.0",
        "@babel/preset-env": "^7.12.0",
        "@types/browserify": "^12.0.36",
        "@types/fs-extra": "^9.0.2",
        "@types/gulp": "^4.0.7",
        "@types/gulp-sourcemaps": "0.0.32",
        "@types/jest": "^26.0.14",
        "@types/request": "^2.48.5",
        "@types/vinyl-buffer": "^1.0.0",
        "@types/vinyl-source-stream": "0.0.30",
        "@typescript-eslint/eslint-plugin": "^4.4.0",
        "@typescript-eslint/parser": "^4.4.0",
        "babelify": "^10.0.0",
        "browserify": "^17.0.0",
        "bufferutil": "^4.0.1",
        "canvas": "^2.6.1",
        "eslint": "^7.11.0",
        "fs-extra": "^9.0.1",
        "gulp": "^4.0.2",
        "gulp-sourcemaps": "^2.6.5",
        "jest": "^26.5.3",
        "jest-circus": "^26.5.3",
        "replace-in-file": "^6.1.0",
        "serve": "^11.3.2",
        "tinyify": "^3.0.0",
        "ts-jest": "^26.4.1",
        "ts-node": "^9.0.0",
        "tsify": "^5.0.2",
        "typescript": "^4.0.3",
        "utf-8-validate": "^5.0.2",
        "vinyl-buffer": "^1.0.1",
        "vinyl-source-stream": "^2.0.0"
    },
    "dependencies": {
        "@types/decompress": "^4.2.3",
        "axios": "^0.20.0",
        "decompress": "^4.2.1"
    }
}
